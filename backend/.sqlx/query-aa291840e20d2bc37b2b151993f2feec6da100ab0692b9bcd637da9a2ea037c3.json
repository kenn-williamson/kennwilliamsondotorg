{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT\n                ar.id,\n                ar.user_id,\n                u.email as user_email,\n                u.display_name as user_display_name,\n                ar.message,\n                ar.requested_role,\n                ar.created_at\n            FROM access_requests ar\n            JOIN users u ON ar.user_id = u.id\n            WHERE ar.status = 'pending'\n            ORDER BY ar.created_at ASC\n            ",
